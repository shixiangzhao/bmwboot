<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shixzh.spring.bmw.dao.VehiclePositionDao">
    <sql id="Base_Column_List">
      `timestamp`, vehicle_id, session_id, lat, lon, heading
    </sql>

    <select id="selectAllSessionsByVehicleId" resultType="com.shixzh.spring.bmw.po.VehiclePositionPO">
        SELECT
        DISTINCT session_id,
        `timestamp`,
        vehicle_id,
        lat,
        lon,
        heading
        FROM vehicle_position
        WHERE vehicle_id = #{vehicleId}
    </select>
</mapper>